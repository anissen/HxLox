module Example.Html for HtmlTest, run

import Std.Dom for Document
import Std.Template for html, safe

fun run () {
  HtmlTest(Document.getElementById('root')).render()
}

class HtmlTest {
  
  new(el) {
    this.el = el
  }

  render() {
    var data = [ 'Hello', 'World' ]
    this.el.innerHTML = html'<div>
      ${ safe(data.map({ it.toLowerCase() }).join(' ')) }
    </div>'
  }

}
